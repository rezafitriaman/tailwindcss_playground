<script>
    function initTopNavigation() {

        return {
            open: false,
            slideNav: false,
            body: document.getElementsByTagName("body")[0],

            noScroll: function() {
                if(this.open) {
                    this.body.classList.add('noScroll');
                }else {
                    this.body.classList.remove('noScroll');
                }
            },

            toggleNav: function (parent) {
                let navConstrain = parent.querySelectorAll('.nav-constrain');
                this.open = ! this.open;
                this.slideNav = false;
                this.noScroll();
                navConstrain.forEach(item=> {
                    if(item.classList.contains('active')) {
                        item.classList.remove('active')
                    }
                })
            },

            toggleSlideNav: function (target) {
                this.slideNav = ! this.slideNav;

                if(this.slideNav) {
                    target.nextElementSibling.classList.add('active');
                }else  {
                    target.closest('.nav-constrain').classList.remove('active');
                }
            }
        };
    }
</script>
